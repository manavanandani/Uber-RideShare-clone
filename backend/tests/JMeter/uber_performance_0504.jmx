          <hashTree/>
          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="Verify Data Volume">
            <stringProp name="BeanShellAssertion.query">
              int driverCount = Integer.parseInt(vars.get("driverCount"));
              int customerCount = Integer.parseInt(vars.get("customerCount"));
              int rideCount = Integer.parseInt(vars.get("rideCount"));
              int billingCount = Integer.parseInt(vars.get("billingCount"));
              
              boolean dataVolumeOK = driverCount >= 1000 && customerCount >= 1000 && 
                                  (rideCount >= 10000 || billingCount >= 10000);
              
              log.info("Data Volume Check: Drivers=" + driverCount + 
                       ", Customers=" + customerCount + 
                       ", Rides=" + rideCount + 
                       ", Billings=" + billingCount);
              
              if (!dataVolumeOK) {
                  log.error("INSUFFICIENT DATA VOLUME FOR PERFORMANCE TESTING!");
                  log.error("Requirements: 10,000 drivers, 10,000 customers, 100,000 billing records");
                  log.error("Run the testDataGenerator script before running performance tests");
                  
                  // Store results for reporting
                  props.put("dataVolumeOK", "false");
                  props.put("driverCount", String.valueOf(driverCount));
                  props.put("customerCount", String.valueOf(customerCount));
                  props.put("rideCount", String.valueOf(rideCount));
                  props.put("billingCount", String.valueOf(billingCount));
                  
                  Failure = true;
                  FailureMessage = "INSUFFICIENT TEST DATA VOLUME: Please run testDataGenerator script";
              } else {
                  log.info("Data volume requirements met. Proceeding with performance tests.");
                  props.put("dataVolumeOK", "true");
                  props.put("driverCount", String.valueOf(driverCount));
                  props.put("customerCount", String.valueOf(customerCount));
                  props.put("rideCount", String.valueOf(rideCount));
                  props.put("billingCount", String.valueOf(billingCount));
